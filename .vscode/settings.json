{
  "typescript.tsdk": "node_modules/typescript/lib",
  "debug.terminal.clearBeforeReusing": true,
  "vsicons.associations.files": [
    {
      "icon": "tsconfig",
      "extensions": [],
      "filenamesGlob": [
        "tsconfig.demos",
      ],
      "extensionsGlob": [
        "json"
      ],
      "filename": true,
      "format": "svg",
    },
    {
      "icon": "rollup",
      "extensions": [],
      "filenamesGlob": [
        "rollup.demos",
      ],
      "extensionsGlob": [ "js" ],
      "filename": true,
      "format": "svg",
    }
  ],
  "cSpell.languageSettings": [
    {
      "languageId": "typescript",
      "allowCompoundWords": false,
      "includeRegExpList": [
        "CStyleComment",
        // "string"
      ],
      // Exclude includes, because they are also strings.
      "ignoreRegExpList": [
        "/from .*/"
      ]
    }
  ],
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.suggestionMenuType": "quickFix",
  "conventionalCommits.scopes": [
    "kernel32",
    "user32",
    "shell32",
    "advapi32",
    "demos",
    "all"
  ],
  "cSpell.words": [
    "advapi",
    "bundable",
    "Koffi",
    "libwin",
    "shakeability"
  ]
}
