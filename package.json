{
  "name": "libwin32",
  "version": "0.1.0",
  "description": "Node bindings to native Win32 DLLs through Koffi",
  "author": "Stephan 'Septh' Schreiber <septh@sfr.fr>",
  "license": "MIT",
  "keywords": [],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./kernel32": "./dist/win32/kernel32.js",
    "./user32": "./dist/win32/user32.js",
    "./rollup-plugin": "./dist/rollup/plugin.js"
  },
  "engines": {
    "node": ">= 18.19.0 || >= 20.6.0"
  },
  "engineStrict": true,
  "files": [
    "dist",
    "!dist/demos",
    "!*.map"
  ],
  "scripts": {
    "build": "tsc",
    "build:demos": "rollup -c source/demos/rollup.config.js",
    "clean": "rimraf .tsbuildinfo dist"
  },
  "dependencies": {
    "koffi": "^2.9.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.5.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.21.2",
    "rollup-plugin-node-externals": "^7.1.3",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4"
  }
}
