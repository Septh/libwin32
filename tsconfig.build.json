{
  "extends": "./tsconfig.json",
  "include": [ "source" ],
  "compilerOptions": {
    "rootDir": "source",
    "outDir": "dist",
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "importHelpers": true,
    "noEmitOnError": true,
    "noEmit": false,
  }
}
