{
  "extends": "./tsconfig.json",
  "include": [ "source" ],
  "exclude": [ "**/zz*" ],
  "compilerOptions": {
    "rootDir": "source",
    "outDir": "lib",
    "removeComments": false,
    "preserveConstEnums": false,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "importHelpers": true,
    "noEmitOnError": true,
    "noEmit": false,
    "stripInternal": true
  }
}
